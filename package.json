{
  "name": "funky",
  "description": "Discord music bot",
  "version": "1.0.0",
  "main": "build/src/main.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node build/src/main.js",
    "serve": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy-cmds": "tsc && node build/src/tools/deploy-guild-commands.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgazz0la/funky.git"
  },
  "keywords": [
    "Discord",
    "music",
    "bot",
    "groovy"
  ],
  "author": "Michael Gazzola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mgazz0la/funky/issues"
  },
  "homepage": "https://github.com/mgazz0la/funky#readme",
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/opus": "^0.5.3",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.6.0",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "ffmpeg-static": "^4.4.0",
    "libsodium-wrappers": "^0.7.9",
    "uuid": "^8.3.2",
    "youtube-search-without-api-key": "^1.0.6",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "@types/memory-cache": "^0.2.1",
    "@types/node-fetch": "2.5.12",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "memory-cache": "^0.2.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  }
}
